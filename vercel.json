{
  "version": 2,
  "buildCommand": "cd backend && npm install && cd ../docs-site && npm install && npm run build",
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "docs-site/build/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/docs/(.*)",
      "dest": "/docs-site/build/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.js"
    }
  ]
}
