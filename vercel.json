{
  "version": 2,
  "buildCommand": "npm install && cd docs-site && npm install && npm run build",
  "outputDirectory": "docs-site/build",
  "rewrites": [
    {
      "source": "/docs",
      "destination": "/index.html"
    },
    {
      "source": "/docs/:path*",
      "destination": "/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/api"
    }
  ]
}
