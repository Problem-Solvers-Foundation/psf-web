{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "docs-site/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/docs",
      "headers": { "Location": "/docs/" },
      "status": 308
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/docs/(.*)",
      "dest": "/docs-site/build/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.js"
    }
  ]
}
