{
  "version": 2,
  "buildCommand": "cd backend && npm install && cd ../docs-site && npm install && npm run build",
  "rewrites": [
    {
      "source": "/docs",
      "destination": "/docs-site/build/index.html"
    },
    {
      "source": "/docs/:path*",
      "destination": "/docs-site/build/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/api"
    }
  ]
}
